// require relevant API
const sendPixel = require('sendPixel');
const log = require('logToConsole');
const JSON = require('JSON');
const encodeUriComponent = require('encodeUriComponent');

// capture values of template fields
const account = data.projectId;
const apiKey = data.apiKey;
const eventType = data.eventType;
const eventValue = data.eventvalue;
const eventData = data.eventData;

// custom domain option (if provided) or use the default domain
const customDomain = data.customDomain || 'beta.usehardal.com';

// use the provided APIs to do things like send pixels
const url =
  'https://' +
  customDomain +
  '/api/ss-collect?projectId=' +
  encodeUriComponent(account) +
  '&apiKey=' +
  encodeUriComponent(apiKey) +
  '&eventType=' +
  encodeUriComponent(eventType) +
  '&eventValue=' +
  encodeUriComponent(eventValue) +
  '&eventData=' + '{' + eventData + '}';

sendPixel(url, data.gtmOnSuccess, data.gtmOnFailure);


